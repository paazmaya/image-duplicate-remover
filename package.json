    "readline-sync": "~1.4.10",
    "tozan": "~6.0.0"
  },
  "devDependencies": {
    "codecov": "3.8.3",
    "eslint": "8.33.0",
    "eslint-config-paazmaya": "9.0.1",
    "eslint-plugin-node": "11.1.0",
    "c8": "7.13.0",
    "tape": "5.6.3"
  }
